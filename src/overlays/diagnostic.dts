/dts-v1/;
/plugin/;

/ {
    fragment@0 {
        target-path = "/";
        __overlay__ {
            emu68 {
                diag {
                    bup: buptest {
                        status = "disabled";
                        iterations = <5>;
                        size = <0x40000>;
                    };
                };
            };
        };
    };

    fragment@1 {
        target-path = "/";
        __overlay__ {
            emu68 {
                diag {
                    mem: membench {
                        status = "disabled";
                        base = <0>;
                        size = <0x40000>;
                    };
                };
            };
        };
    };

    __overrides__ {
        buptest = <&bup>,"status";
        bupiter = <&bup>,"iterations:0";
        bupsize = <&bup>,"size:0";

        membench = <&mem>,"status";
        membase = <&mem>,"base:0";
        memsize = <&mem>,"size:0";
    };
};
